<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FFA1887F</item>
        <item name="colorPrimaryDark">#666565</item>
        <item name="colorAccent">#000000</item>
    </style>
    <style name="AppThemed" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#666565</item>
        <item name="android:actionOverflowButtonStyle">@style/MyActionButtonOverflow</item>
        <item name="colorAccent">#000000</item>
    </style>
    <style name="MyActionButtonOverflow" parent="android:style/Widget.Holo.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_filter_list_white_24dp</item>
        <item name="android:background">?android:attr/actionBarItemBackground</item>
        <item name="android:contentDescription">"Lala"</item>
    </style>

    <style name="AppThemed2" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#666565</item>
        <item name="colorAccent">#11dcad</item>
    </style>


    <style name="animationName" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/zoom_ind</item>
        <item name="android:windowExitAnimation">@anim/zoom_out</item>
    </style>

</resources>
